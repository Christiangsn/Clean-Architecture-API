{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-new": "off",
        "no-prototype-builtins": "off",
        "no-restricted-syntax": "off",
        "max-classes-per-file": "off",
        "@typescript-eslint/no-explicit-any": "off",
        "no-console": "off",
        "import/prefer-default-export": "off",
        "no-useless-constructor": "off"
    }
}

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id              String       @id       @default(uuid())
  name            String       
  email           String
  password        String
  accessToken     String?

  @@map("users")
}

model logsError {
  id              String       @id       @default(uuid())
  stack           String   
  created_at      DateTime               @default(now())

  @@map("log_erros")
}